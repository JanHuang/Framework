{
    "name": "fastd/framework",
    "type": "library",
    "license": "MIT",
    "description": "The fastd framework library.",
    "minimum-stability": "dev",
    "keywords": [
        "FastD",
        "FastD Framework",
        "FastD Module",
        "FastD Bootstrap"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src/FastD"
        }
    },
    "require": {
        "php": ">=5.5",
        "fastd/http": "@dev",
        "fastd/routing": "@dev",
        "fastd/database": "@dev",
        "fastd/container": "@dev",
        "fastd/config": "@dev",
        "fastd/storage": "@dev",
        "fastd/logger": "@dev",
        "fastd/console": "@dev",
        "fastd/finder": "@dev",
        "fastd/debug": "@dev",
        "fastd/template": "@dev"
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
