{
    "name": "fastd/framework",
    "type": "library",
    "license": "MIT",
    "description": "The fastd framework library.",
    "minimum-stability": "stable",
    "keywords": [
        "FastD",
        "FastD Framework",
        "FastD Module",
        "FastD Bootstrap"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src/FastD"
        }
    },
    "require": {
        "php": ">=5.5",
        "fastd/http": "1.0.*@BETA",
        "fastd/routing": "~0.1.1",
        "fastd/database": "~0.2.0",
        "fastd/container": "~0.1.1",
        "fastd/config": "~1.0.0",
        "fastd/storage": "~0.1.0",
        "fastd/logger": "~0.1.0",
        "fastd/console": "~0.1.3",
        "fastd/finder": "~1.0.0",
        "fastd/debug": "~1.0.1",
        "fastd/template": "~0.1.0"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
