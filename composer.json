{
    "name": "fastd/framework",
    "type": "library",
    "license": "MIT",
    "description": "The fastd framework library.",
    "minimum-stability": "stable",
    "keywords": [
        "FastD",
        "FastD Framework",
        "FastD Module",
        "FastD Bootstrap"
    ],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src/FastD"
        }
    },
    "require": {
        "php": ">=5.5",
        "fastd/http": "~0.1.0",
        "fastd/routing": "~0.1.1",
        "fastd/database": "~0.1.2",
        "fastd/container": "~0.1.1",
        "fastd/config": "~0.1.0",
        "fastd/storage": "~0.1.0",
        "fastd/logger": "~0.1.0",
        "fastd/console": "~0.1.2",
        "fastd/finder": "~0.1.0",
        "fastd/debug": "~0.1.0",
        "fastd/template": "~0.1.0"
    },
    "require-dev": {
        "fastd/fastd-module": "dev-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "scripts": {
        "post-update-cmd": "Module\\Module::init",
        "post-install-cmd": "Module\\Module::init",
        "post-create-project-cmd": "Module\\Module::init"
    }
}
