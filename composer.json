{
    "name": "fastd/framework",
    "type": "library",
    "license": "MIT",
    "description": "The fastd framework library.",
    "minimum-stability": "stable",
    "keywords": ["FastD Framework Kernel"],
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FastD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "app/src/Bundles"
        },
        "files": [
            "app/application.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "fastd/http": "2.0.x-dev",
        "fastd/debug": "2.0.x-dev",
        "fastd/config": "2.0.x-dev",
        "fastd/console": "2.0.x-dev",
        "fastd/routing": "2.0.x-dev",
        "fastd/storage": "2.0.x-dev",
        "fastd/database": "2.0.x-dev",
        "fastd/container": "2.0.x-dev",
        "fastd/annotation": "2.0.x-dev",
        "fastd/generator": "~1.0",
        "symfony/finder": "~3.0",
        "twig/twig": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "bin": [
        "console"
    ]
}
